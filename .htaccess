
#remove html file extension https://fargofoodies.com/index.html
# to https://fargofoodies.com/Home
#remove html file extension https://fargofoodies.com/.html
# to https://fargofoodies.com/Events
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]